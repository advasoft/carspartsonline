<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing" x:Class="StoreAppTest.Print.ReportsView"
    mc:Ignorable="d"
    d:DesignHeight="600" d:DesignWidth="800">

    <Grid x:Name="LayoutRoot" Background="White">

        <dxp:DocumentPreview>
            <dxp:DocumentPreview.Model>
                <dxp:ReportPreviewModel AutoShowParametersPanel="False"></dxp:ReportPreviewModel>
            </dxp:DocumentPreview.Model>
        </dxp:DocumentPreview>

    </Grid>
</UserControl>
