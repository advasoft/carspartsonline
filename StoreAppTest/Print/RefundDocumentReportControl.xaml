<sdk:ChildWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    x:Class="StoreAppTest.Print.RefundDocumentReportControl"
    mc:Ignorable="d"
    d:DesignHeight="600" d:DesignWidth="900" AllowDrop="True" Loaded="ChildWindow_Loaded" >

    <Grid x:Name="LayoutRoot" Background="White">

        <dxp:DocumentPreview  Name="DocumentPreview">
            <dxp:DocumentPreview.Model>
                <dxp:ReportPreviewModel x:Name="PreviewModel" AutoShowParametersPanel="False" DefaultPrintMode="Native" ReportName="StoreAppTest.Reports.RefundDocumentReport, StoreAppTest.Reports, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" ServiceUri="../StoreAppReportService.svc" IsParametersPanelVisible="False" ></dxp:ReportPreviewModel>
            </dxp:DocumentPreview.Model>
        </dxp:DocumentPreview>

    </Grid>
</sdk:ChildWindow>
